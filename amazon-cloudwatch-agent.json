{
  "agent": {
    "metrics_collection_interval": 60,
    "logfile": "/home/ec2-user/logs/cloudwatch/amazon-cloudwatch-agent.log"
  },
  "logs": {
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/home/ec2-user/logs/fixity.log",
            "log_group_name": "fixity-prod-log",
            "log_stream_name": "fixity-prod-log",
            "timezone": "Local"
          }
        ]
      }
    },
    "log_stream_name": "fixity-prod-log"
  },
  "metrics": {
    "append_dimensions": {
      "InstanceId":"${aws:InstanceId}"
    },
    "metrics_collected": {
      "mem": {
        "measurement": [
          "available_percent"
        ]
      }
    }
  }
}
